{
  "name": "Pokeshot Autotranslate",
  "title": "Pokeshot Autotranslate App",
  "description": "Adds Pokeshot Autotranslate support to IBM Connections Homepage POC",
  "services": [
    "Customizer"
  ],
  "extensions": [
    {
      "payload": {
        "include-files": [
          "jquery-3.3.1.min.js",
          "mock_soyhook.js",
          "smz-translation-selectors.js",
          "jquery.smz-translate.js",
          "jquery.smz-translate-auto.js",
          "smz-translate.css"
        ],
        "include-repo":{
          "name": "ibm-connections-translation"
        },
        "cache-headers": {
          "cache-control": "max-age=0"
